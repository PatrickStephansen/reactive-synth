{
  "name": "reactive-synth",
  "version": "0.0.0",
  "author": {
    "name": "Patrick Stephansen"
  },
  "scripts": {
    "ng": "ng",
    "start": "webpack --config=webpack.worklets.dev.js && node get-worklets-url.js > src/app/cache-hack/cache.ts && ng serve",
    "build": "webpack --config=webpack.worklets.production.js && node get-worklets-url.js > src/app/cache-hack/cache.ts && ng run reactive-synth:build:production --base-href /reactive-synth/",
    "watch:worklets": "webpack --config=webpack.worklets.dev.js --watch",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build && gh-pages -d dist/reactive-synth",
    "postinstall": "ngcc && npm link reactive-synth-bitcrusher reactive-synth-inverse-gain reactive-synth-noise-generator reactive-synth-clock-divider reactive-synth-envelope-generator"
  },
  "description": "A modular syth made as an exercise to learn ng-rx",
  "homepage": "https://PatrickStephansen.github.io/reactive-synth",
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.5",
    "@angular/common": "^13.2.5",
    "@angular/compiler": "^13.2.5",
    "@angular/core": "^13.2.5",
    "@angular/forms": "^13.2.5",
    "@angular/platform-browser": "^13.2.5",
    "@angular/platform-browser-dynamic": "^13.2.5",
    "@angular/router": "^13.2.5",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "bulma": "^0.9.3",
    "core-js": "^3.21.1",
    "ng-click-outside": "^9.0.1",
    "ramda": "^0.28.0",
    "rxjs": "^7.5.4",
    "standardized-audio-context": "^19.1.1",
    "tslib": "^2.3.1",
    "yup": "^0.32.11",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.5",
    "@angular/cli": "^13.2.5",
    "@angular/compiler-cli": "^13.2.5",
    "@angular/language-service": "^13.2.5",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/jest": "^27.4.1",
    "@types/node": "^12.20.7",
    "clean-webpack-plugin": "^3.0.0",
    "codelyzer": "^6.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^2.2.0",
    "jest": "^27.0.0",
    "jest-preset-angular": "^11.1.1",
    "prettier": "^1.19.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.5.5",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2"
  }
}
