{
  "name": "reactive-synth",
  "version": "0.0.0",
  "author": {
    "name": "Patrick Stephansen"
  },
  "scripts": {
    "ng": "ng",
    "start": "webpack --config=webpack.worklets.dev.js && ng serve",
    "build": "webpack --config=webpack.worklets.production.js && node get-worklets-url.js > src/app/cache-hack/cache.prod.ts && ng run reactive-synth:build:production --base-href /reactive-synth/",
    "watch:worklets": "webpack --config=webpack.worklets.dev.js --watch",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build && gh-pages -d dist/reactive-synth"
  },
  "description": "A modular syth made as an exercise to learn ng-rx",
  "homepage": "https://PatrickStephansen.github.io/reactive-synth",
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "bulma": "^0.7.5",
    "core-js": "^2.6.11",
    "ng-click-outside": "^6.0.0",
    "ramda": "^0.26.1",
    "rxjs": "^6.5.5",
    "standardized-audio-context": "^19.1.1",
    "tslib": "^1.13.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.3",
    "@angular/cli": "^8.3.26",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "babel-jest": "^24.9.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "codelyzer": "^5.2.2",
    "gh-pages": "^2.2.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "transform": {
      "\\.[jt]s$": "babel-jest"
    }
  }
}
