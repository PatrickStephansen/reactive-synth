{
  "name": "reactive-synth",
  "version": "0.0.0",
  "author": {
    "name": "Patrick Stephansen"
  },
  "scripts": {
    "ng": "ng",
    "start": "webpack --config=webpack.worklets.dev.js && ng serve",
    "build": "webpack --config=webpack.worklets.production.js && node get-worklets-url.js > src/app/cache-hack/cache.prod.ts && ng run reactive-synth:build:production --base-href /reactive-synth/",
    "watch:worklets": "webpack --config=webpack.worklets.dev.js --watch",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build && gh-pages -d dist/reactive-synth"
  },
  "description": "A modular syth made as an exercise to learn ng-rx",
  "homepage": "https://PatrickStephansen.github.io/reactive-synth",
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.0",
    "@angular/common": "^8.2.0",
    "@angular/compiler": "^8.2.0",
    "@angular/core": "^8.2.0",
    "@angular/forms": "^8.2.0",
    "@angular/platform-browser": "^8.2.0",
    "@angular/platform-browser-dynamic": "^8.2.0",
    "@angular/router": "^8.2.0",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@ngrx/effects": "^8.2.0",
    "@ngrx/store": "^8.2.0",
    "@ngrx/store-devtools": "^8.2.0",
    "bulma": "^0.7.5",
    "core-js": "^2.6.9",
    "ng-click-outside": "^4.0.0",
    "ramda": "^0.26.1",
    "rxjs": "~6.5.2",
    "standardized-audio-context": "^19.1.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular/cli": "^8.2.0",
    "@angular/compiler-cli": "^8.2.0",
    "@angular/language-service": "^8.2.0",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "babel-jest": "^24.8.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "codelyzer": "^5.0.1",
    "gh-pages": "^2.0.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1"
  },
  "jest": {
    "transform": {
      "\\.[jt]s$": "babel-jest"
    }
  }
}
