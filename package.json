{
  "name": "reactive-synth",
  "version": "0.0.0",
  "author": {
    "name": "Patrick Stephansen"
  },
  "scripts": {
    "ng": "ng",
    "start": "webpack --config=webpack.worklets.js && ng serve",
    "build": "webpack --config=webpack.worklets.js && ng run reactive-synth:build:production --base-href /reactive-synth/",
    "watch:worklets": "webpack --config=webpack.worklets.js --watch",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build && gh-pages -d dist/reactive-synth"
  },
  "description": "A modular syth made as an exercise to learn ng-rx",
  "homepage": "https://PatrickStephansen.github.io/reactive-synth",
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.13",
    "@angular/common": "^7.2.13",
    "@angular/compiler": "^7.2.13",
    "@angular/core": "^7.2.13",
    "@angular/forms": "^7.2.13",
    "@angular/http": "^7.2.13",
    "@angular/platform-browser": "^7.2.13",
    "@angular/platform-browser-dynamic": "^7.2.13",
    "@angular/router": "^7.2.13",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/store": "^7.4.0",
    "@ngrx/store-devtools": "^7.4.0",
    "bulma": "^0.7.4",
    "core-js": "^2.6.5",
    "ramda": "^0.26.1",
    "rxjs": "~6.4.0",
    "standardized-audio-context": "^19.0.3",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.13",
    "@angular/language-service": "^7.2.13",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "babel-jest": "^24.5.0",
    "babel-preset-env": "^1.7.0",
    "codelyzer": "~4.5.0",
    "gh-pages": "^2.0.1",
    "jest": "^24.5.0",
    "prettier": "^1.16.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.4",
    "webpack-cli": "^3.2.3"
  },
  "jest": {
    "transform": {
      "\\.js$": "babel-jest"
    }
  }
}
