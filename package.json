{
  "name": "reactive-synth",
  "version": "0.0.0",
  "author": {
    "name": "Patrick Stephansen"
  },
  "scripts": {
    "ng": "ng",
    "start": "webpack --config=webpack.worklets.js && ng serve",
    "build": "webpack --config=webpack.worklets.js && ng run reactive-synth:build:production --base-href /reactive-synth/",
    "watch:worklets": "webpack --config=webpack.worklets.js --watch",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build && gh-pages -d dist/reactive-synth"
  },
  "description": "A modular syth made as an exercise to learn ng-rx",
  "homepage": "https://PatrickStephansen.github.io/reactive-synth",
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.3",
    "@angular/common": "^8.1.3",
    "@angular/compiler": "^8.1.3",
    "@angular/core": "^8.1.3",
    "@angular/forms": "^8.1.3",
    "@angular/platform-browser": "^8.1.3",
    "@angular/platform-browser-dynamic": "^8.1.3",
    "@angular/router": "^8.1.3",
    "@ngrx/effects": "^8.1.0",
    "@ngrx/store": "^8.1.0",
    "@ngrx/store-devtools": "^8.1.0",
    "bulma": "^0.7.5",
    "core-js": "^2.6.9",
    "ramda": "^0.26.1",
    "rxjs": "~6.5.2",
    "standardized-audio-context": "^19.1.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular/cli": "^8.1.2",
    "@angular/compiler-cli": "^8.1.3",
    "@angular/language-service": "^8.1.3",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "babel-jest": "^24.8.0",
    "babel-preset-env": "^1.7.0",
    "codelyzer": "^5.0.1",
    "gh-pages": "^2.0.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5",
    "webpack-cli": "^3.3.6"
  },
  "jest": {
    "transform": {
      "\\.js$": "babel-jest"
    }
  }
}